Proceso sin_titulo
	Definir numero Como Entero;
	Escribir "MOSTRAR SI UN NÚMERO ES PAR O IMPAR USANDO OPERADOR MOD.";
    Escribir " ";
    Escribir Sin Saltar "Ingrese Número : ";
    Leer numero;
    Si (numero MOD 2) == 0 Entonces
        Escribir "NÚMERO PAR";
    SiNo
        Escribir "NÚMERO IMPAR";
    FinSi
FinProceso
