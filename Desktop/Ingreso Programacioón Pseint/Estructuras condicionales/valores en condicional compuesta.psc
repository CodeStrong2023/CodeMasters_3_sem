Algoritmo sin_titulo
	Definir valor1,valor2 Como Entero;
	Escribir "Ingresar el valor 1:";
	Leer valor1;
	Escribir "Ingresar el segundo valor:";
	Leer valor2;
	si valor1>= valor2 Entonces
		Escribir "La cantidad de personas es suficientes:";
	sino 
		Escribir "La cantidad de personas no es suficientes:";
		
	FinSi
FinAlgoritmo
